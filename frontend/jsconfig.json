{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@cfg/*": ["./cfg/*"],
      "@services/*": ["./src/services/*"],
      "@components/*": ["./src/components/*"],
      "@views/*": ["./src/views/*"],
      "@assets/*": ["./src/assets/*"],
      "@store/*": ["./src/store/*"],
    }
  },
  "include": ["src/**/*", "cfg/**/*"],
  "exclude": ["node_modules", "dist"]
}