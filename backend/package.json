{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/app.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "dev:js": "nodemon src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:seed": "ts-node -r tsconfig-paths/register scripts/seedUsers.ts",
    "db:seed-stickers": "ts-node -r tsconfig-paths/register scripts/seedStickerPacks.ts",
    "db:indexes": "ts-node -r tsconfig-paths/register scripts/createIndexes.ts",
    "db:setup": "npm run db:indexes && npm run db:seed && npm run db:seed-stickers",
    "db:reset": "ts-node -r tsconfig-paths/register scripts/createIndexes.ts drop && npm run db:setup",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.5",
    "mongoose": "^8.15.1",
    "multer": "^2.0.0",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.55",
    "@types/node-cache": "^4.1.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
